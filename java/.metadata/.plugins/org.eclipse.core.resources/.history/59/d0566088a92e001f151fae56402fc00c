package jaunty.struts.orm;

/**
 * AbstractSecurityUser entity provides the base persistence definition of the
 * SecurityUser entity. @author MyEclipse Persistence Tools
 */

public abstract class AbstractSecurityUser implements java.io.Serializable {

	// Fields

	private Long userId;
	private String username;
	private String password;

	// Constructors

	/** default constructor */
	public AbstractSecurityUser() {
	}

	/** full constructor */
	public AbstractSecurityUser(String username, String password) {
		this.username = username;
		this.password = password;
	}

	// Property accessors

	public Long getUserId() {
		return this.userId;
	}

	public void setUserId(Long userId) {
		this.userId = userId;
	}

	public String getUsername() {
		return this.username;
	}

	public void setUsername(String username) {
		this.username = username;
	}

	public String getPassword() {
		return this.password;
	}

	public void setPassword(String password) {
		this.password = password;
	}

}
package jaunty.struts.orm;



/**
 * AbstractSecurityRole entity provides the base persistence definition of the SecurityRole entity. @author MyEclipse Persistence Tools
 */

public abstract class AbstractSecurityRole  implements java.io.Serializable {


    // Fields    

     private Integer roleId;
     private String roleName;


    // Constructors

    /** default constructor */
    public AbstractSecurityRole() {
    }

    
    /** full constructor */
    public AbstractSecurityRole(String roleName) {
        this.roleName = roleName;
    }

   
    // Property accessors

    public Integer getRoleId() {
        return this.roleId;
    }
    
    public void setRoleId(Integer roleId) {
        this.roleId = roleId;
    }

    public String getRoleName() {
        return this.roleName;
    }
    
    public void setRoleName(String roleName) {
        this.roleName = roleName;
    }
   








}